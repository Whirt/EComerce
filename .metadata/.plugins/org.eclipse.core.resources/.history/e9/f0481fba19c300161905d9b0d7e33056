package IO;

public abstract class ErrorValue {
	
	public static final int IO_LOGIN_ERROR = -1 ;
	public static final int IO_CREATING_USER_ERROR = -2 ;
	public static final int PRODUCT_ILLEGAL_ARGUMENT_ERROR = -3 ;
	public static final int IO_PRODUCT_FAILED_TO_LOAD_ERROR = -4 ;
	public static final int IO_PRODUCT_FAILED_TO_SAVE_ERROR = -5 ;
	public static final int IO_PRODUCT_DOESN_EXIST = -6 ;
	
}
