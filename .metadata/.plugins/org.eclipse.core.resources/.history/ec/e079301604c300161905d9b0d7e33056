package mainframe;

import mainpanel.AdminPanel;
import mainpanel.Panel;
import user.User;

public class AdminFrame extends Frame {
	
	public AdminFrame(User user, Panel panel) {
		super(user) ;
		panel.setHeaderName(user.getUsername()) ;
		setContentPane(panel) ;
	}
	private AdminFrame() { ; }
}
